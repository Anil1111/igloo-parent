							<artifactId>wagon-ssh-external</artifactId>
							<version>${owsi-core.wagon-ssh-external-plugin.version}</version>
						</dependency>
					</dependencies>
				</plugin>
			</plugins>
		</pluginManagement>
	</build>
	
